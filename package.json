{
  "name": "node-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "scripts": {
    "dev": "DEBUG=api* ts-node-dev -r dotenv/config -r tsconfig-paths/register --transpile-only --inspect --no-notify src/shared/server/express/server.ts",
    "start": "DEBUG=api* node -r dotenv/config build/shared/server/express/server.js",
    "test": "jest --clearCache && jest --passWithNoTests",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "build": "ttsc",
    "migrate": "DEBUG=api* ts-node-dev -r dotenv/config -r tsconfig-paths/register --transpile-only --inspect --no-notify src/migration-script/start.ts"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.5.0",
    "@mikro-orm/mongodb": "^4.4.4",
    "@types/bluebird": "^3.5.34",
    "aws-sdk": "^2.843.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "celebrate": "^13.0.4",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "db-migrate": "^0.11.12",
    "db-migrate-mysql": "^2.1.2",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "global": "^4.4.0",
    "googleapis": "^73.0.0",
    "ioredis": "^4.23.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "pm2": "^4.5.6",
    "reflect-metadata": "^0.1.13",
    "run-rs": "^0.7.4",
    "tsyringe": "^4.4.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/multer-s3": "^2.7.9",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "add": "^2.0.6",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typeorm": "^0.2.32",
    "typescript": "^4.1.3",
    "typescript-transform-paths": "^2.2.2",
    "yarn": "^1.22.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
